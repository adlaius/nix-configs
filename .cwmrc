sticky yes

bind CM-Return unmap
bind CM-Delete unmap
bind M-Return unmap
bind M-Down unmap
bind M-Up unmap
bind M-slash unmap
bind C-slash unmap
bind CM-n unmap
bind CM-x unmap
bind CM-1 unmap
bind CM-2 unmap
bind CM-3 unmap
bind CM-4 unmap
bind CM-5 unmap
bind CM-6 unmap
bind CM-7 unmap
bind CM-8 unmap
bind CM-9 unmap
bind CM-0 unmap
bind CM-g unmap
bind M-Right unmap
bind M-Left unmap
bind CM-f unmap
bind CM-equal unmap
bind CMS-equal unmap
bind M-question unmap
bind M-period unmap
bind CM-w unmap
bind CMS-r unmap
bind CMS-q unmap

bind 4-Return terminal
bind 4-l lock
bind 4-slash search
bind 4-question exec
bind 4-f maximize
bind 4-equal vmaximize
bind 4S-equal hmaximize
bind 4-g grouptoggle
bind 4-r reload
bind 4S-q quit
bind 4-w exec_wm

bind 4-1 grouponly1
bind 4-2 grouponly2
bind 4-3 grouponly3
bind 4S-1 movetogroup1
bind 4S-2 movetogroup2
bind 4S-3 movetogroup3

command chromium "chromium --proxy-server=127.0.0.1:8118"
command htop "xterm -fg lightyellow -bg gray20 -e htop"
command dark_xterm "xterm"
command light_xterm "xterm -fg rgb:00/10/2a -bg lightyellow -fa AnonymousPro-10"
