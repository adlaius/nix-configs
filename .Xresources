Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.dpi: 96
Xft.rgba: rgb

XTerm*metaSendsEscape: true
XTerm*scrollBar: false
Emacs.font: monospace-10

!! light terminal colors
*foreground: black
*background: white

!black
*color0:  black
*color8:  gray30

!red
*color1:  red4
*color9:  red2

!green
*color2:  darkgreen
*color10: forestgreen

!yellow
*color3:  goldenrod
*color11: chocolate

!blue
*color4:  mediumblue
*color12: darkblue

!magenta
*color5:  darkviolet
*color13: darkmagenta

!cyan
*color6:  cyan4
*color14: darkcyan

!white
*color7:  gray85
*color15: gray50


