! -*- Conf-Xdefaults -*-

! N.B. to reset keyboard layout, run 'setxkbmap'

! emulate Symbolics "space cadet" keyboard layout
! (LTR: ALT_L, CTRL_L, SPC, CTRL_R, ALT_R) on ASUS laptop
clear control
clear mod1
clear mod4
clear mod5
keycode 64 = Control_L
keycode 133 = Alt_L
keycode 108 = Control_R
keycode 135 = Alt_R
add control = Control_L Control_R
add mod1 = Alt_L Alt_R

! The following is said to work on Win32 via AutoHotKeys (untested)
! LCtrl::LWin
